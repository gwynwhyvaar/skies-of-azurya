<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <appSettings>
    <!--Default model to load at start up. Values can only be :nazvhy|arch-mage|chairon|black-king-->
    <add key="DefaultModel" value="nazvhi-flight" />
    <add key="DefaultBackground" value="nazvhi-background" />
    <!--Layer depth for the Background Image. must be a value between 0 and 1-->
    <add key="LayerDepth" value="0" />
    <!--Camera Positions for x,y,and z axis. default values are 0.0f, 50.0f, 5000.0f-->
    <add key="CameraPositionX" value="0" />
    <add key="CameraPositionY" value="0" />
    <add key="CameraPositionZ" value="25000" />
    <!--Game Constants-->
    <add key="CameraHeight" value="25000.0" />
    <add key="PlayFieldSizeX" value="16000.0" />
    <add key="PlayFieldSizeY" value="12500.0" />
    <add key="NumberOfObstacles" value="10" />
    <add key="ObstacleMinimuSpeed" value="100" />
    <add key="ObstacleMaximumSpeed" value="300" />
    <add key="SpeedAdjustment" value="5.0" />
    <!--Setting for Multisampling. true|false-->
    <add key="AllowMultiSampling" value="true" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
</configuration>
